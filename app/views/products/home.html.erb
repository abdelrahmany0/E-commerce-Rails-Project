          font-weight: bold;
          transition: color 0.5s;
      }
      .member-details h6 {
          font-size: 0.85rem;
      }
      #our-team .overlay {
          position: absolute;
          height: 100%;
          width: 100%;
          transition: 0.5s background-color;
      }
      #our-team .layer img {
          transition: transform 0.7s;
      }
      #our-team .layer:hover .overlay {
          top: 0;
          background-color: rgba(247, 123, 123, 0.85);
      }
      #our-team .layer:hover img {
          transform: scale(1.1);
      }
      #our-team .layer:hover .member-details .member-name {
          color: #f25454;
      }

      .contact-us {
          background-color: #f25454;
          transition: background-color 0.5s;
      }
      .contact-us:hover {
          background-color: #f38383;
      }
      /* End Our Team Section  */

      /* Start Services Section  */
      .sevice-item-head {
          transition: color 0.5s;
      }
      .item:hover .sevice-item-head {
          color: #f25454;
      }

      /* End Services Section  */

      /* Start Contact Section  */
