  <style>
      h2::after {
          display: block;
          height: 1px;
          background-color: #000;
          content: " ";
          margin: 1rem auto;
          width: 50px;
          transition: width 0.4s;
      }
      .heading:hover h2:after {
          width: 90px;
      }

      .icon-color {
          color: #f25454;
      }
      p {
          font-size: 1.1rem;
      }
      h3 {
          font-size: 1.3rem;
      }

      /******  End Main Rules ************/

      /* Start Header Section  */

      .carousel-caption {
          position: absolute;
          top: 50%;
          left: 50%;
          z-index: 10;
          padding-top: 20px;
          padding-bottom: 20px;
          color: #fff;
          text-align: center;
          transform: translate(-50%, -50%);
      }
      header .font-weight-bolder {
          font-size: 5rem;
      }
      .carousel-item img {
          height: 100vh;
      }

      #carouselExampleCaptions .carousel-control-prev {
          position: absolute;
          top: 50%;
          left: 0;
          width: 50px;
          height: 50px;
          transform: translateY(-50%);
          border: 1px solid #fff;
          z-index: 11;
      }
      #carouselExampleCaptions .carousel-control-next {
          position: absolute;
          top: calc(50% + 50px);
          left: 0px;
          width: 50px;
          height: 50px;
          transform: translateY(-50%);
          border: 1px solid #fff;
          border-top: none;
          z-index: 11;
      }

      .carousel-icon:hover {
          background-color: #fff;
          color: #f25454 !important;
      }

      /* End Header Section  */
      /* Start About Section  */
      .heading {
          position: relative;
      }
      .heading h2 {
          position: absolute;
          left: 50%;
          top: 0;
          transform: translate(-50%, 20px);
      }
      .heading h3 {
          font-size: 3rem;
          color: #e9e7e7;
      }
      .progress {
          height: 4px;
      }
      .progress-head {
          font-size: 0.9rem;
          font-weight: 750;
      }
      .progress-bar {
          background-color: #f25454;
          height: 4px;
      }
      /* End About Section  */

      /* Start Our Team Section  */
